<script>
    $(document).ready(function () {
        var triggerClick = 0;
        $("#fullScreenMap").click(function () {
            if (triggerClick == 0) {
                triggerClick = 1;
                $("#googleMap").animate({ height: '600px' }, 500);
                document.getElementById("fullScreenMap").setAttribute("class", "btn btn-default map-controller-btn mini-screen-btn");
            } else {
                triggerClick = 0;
                $("#googleMap").animate({ height: '380px' }, 500);
                document.getElementById("fullScreenMap").setAttribute("class", "btn btn-default map-controller-btn full-screen-btn");
            };
        });
    });
</script>

